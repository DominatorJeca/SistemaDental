<Window x:Class="SistemaDental.Ajustes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SistemaDental"
        mc:Ignorable="d"
        Title="Ajustes" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="bienvenida.png"/>
        </Grid.Background>
        <Button x:Name="btnEditarUsuario" Content="Editar Usuario" HorizontalAlignment="Left" Margin="306,125,0,0" VerticalAlignment="Top" Width="224" Background="#FF056676" BorderBrush="{x:Null}"/>
        <Button x:Name="btnAgregarNuevoUsuario" Content="Agregar Nuevo Usuario" HorizontalAlignment="Left" Margin="306,178,0,0" VerticalAlignment="Top" Width="224" Background="#FF056676" BorderBrush="{x:Null}"/>
        <Button x:Name="btnManejarUsuarios" Content="Manejar Usuarios" HorizontalAlignment="Left" Margin="306,229,0,0" VerticalAlignment="Top" Width="224" Background="#FF056676" BorderBrush="{x:Null}"/>
        <Button x:Name="btnRegresar" Content="Regresar" HorizontalAlignment="Left" Margin="306,280,0,0" VerticalAlignment="Top" Width="224" Background="#FF056676" BorderBrush="{x:Null}"/>
    </Grid>
</Window>
