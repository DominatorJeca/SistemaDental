<UserControl x:Class="SistemaDental.MVCCV.Vista.MenuReporteVista"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SistemaDental.MVCCV.Vista"
             mc:Ignorable="d" 
             d:DesignHeight="455" d:DesignWidth="1050">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/imagenes/TeethPattern.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <Button x:Name="btnreporte1" Content="Reporte 1" HorizontalAlignment="Center" Margin="0,125,0,0" VerticalAlignment="Top" Width="224" Background="#FF056676" BorderBrush="{x:Null}" />
        <Button x:Name="btnreporte2" Content="Reporte 2" HorizontalAlignment="Center" Margin="0,175,0,0" VerticalAlignment="Top" Width="224" Background="#FF056676" BorderBrush="{x:Null}" />
        <Button x:Name="btnreporte3" Content="Reporte 3" HorizontalAlignment="Center" Margin="0,225,0,0" VerticalAlignment="Top" Width="224" Background="#FF056676" BorderBrush="{x:Null}" />
        <Button x:Name="btnreporte4" Content="Reporte 4" HorizontalAlignment="Center" Margin="0,276,0,0" VerticalAlignment="Top" Width="224" Background="#FF056676" BorderBrush="{x:Null}" />
        <Button x:Name="btnreporte5" Content="Reporte 5" HorizontalAlignment="Center" Margin="0,328,0,0" VerticalAlignment="Top" Width="224" Background="#FF056676" BorderBrush="{x:Null}" />
    </Grid>
</UserControl>
