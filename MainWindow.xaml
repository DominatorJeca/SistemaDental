<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SistemaDental"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" x:Class="SistemaDental.MainWindow"
        mc:Ignorable="d"
        Title="Bienvenido" Height="340" Width="630" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Window.Background>
        <ImageBrush ImageSource="bienvenida.png"/>
    </Window.Background>
    <Grid Margin="10,10,0,0">
        <Button Content="Iniciar" HorizontalAlignment="Left" Margin="265,163,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Background="#FF056676" Foreground="White" BorderBrush="{x:Null}"/>
        <Label Content="Bienvenido" HorizontalAlignment="Left" Margin="163,110,0,0" VerticalAlignment="Top" FontSize="32" FontFamily="Bahnschrift"/>
        <Label x:Name="lblUsuario" Content="Usuario" HorizontalAlignment="Left" Margin="328,110,0,0" VerticalAlignment="Top" FontSize="32" FontFamily="Bahnschrift"/>
        <Label x:Name="lblFecha" Content="Fecha" HorizontalAlignment="Left" Margin="540,-6,0,0" VerticalAlignment="Top" FontSize="16" FontFamily="Bahnschrift" Height="33"/>
    </Grid>
</Window>
