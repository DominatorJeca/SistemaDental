  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@mes">
            <Value>=Parameters!mes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@anio">
            <Value>=Parameters!anio.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>set Language Spanish
if @mes!=0
select DATENAME(MONTH,FechaCompra)as Mes, SUM(PrecioCompra) as Total  from Compra
inner join DetalleCompra on Compra.CompraID=DetalleCompra.FKCompraID
where YEAR(FechaCompra)in (@anio) and MONTH(FechaCompra) in (@mes)
group by FechaCompra

if @mes=0
select DATENAME(MONTH,FechaCompra)as Mes, SUM(PrecioCompra) as Total  from Compra 
inner join DetalleCompra on Compra.CompraID=DetalleCompra.FKCompraID
where YEAR(FechaCompra) in (@anio)
group by FechaCompra</CommandText>
      </Query>
